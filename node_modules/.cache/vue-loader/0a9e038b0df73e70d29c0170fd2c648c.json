r\n//点击回退逻辑\r\nconst useBackEffect = () => {\r\n  const router = useRouter();\r\n  const handelBackClick = () => {\r\n    // 退回上一个\r\n    router.back();\r\n  }\r\n  return handelBackClick\r\n}\r\nexport default {\r\n  name: 'Shop',\r\n  components: { ShopInfo },\r\n  setup () {\r\n    // 函数的执行流程\r\n    const { item, getItemData } = useShopInfoEffect();\r\n    const handelBackClick = useBackEffect();\r\n    getItemData(item);\r\n    return { handelBackClick }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n@import \"../../style/viriables.scss\";\r\n.wrapper {\r\n  padding: 0 0.18rem;\r\n}\r\n.search {\r\n  display: flex;\r\n  margin: 0.14rem 0 0.1rem 0;\r\n  line-height: 0.2rem;\r\n  .iconfont {\r\n    font-size: 0.16rem;\r\n  }\r\n  &__content {\r\n    display: flex;\r\n    flex: 1;\r\n    background: $content-bgColor;\r\n    border-radius: 0.16rem;\r\n  }\r\n}\r\n</style>"]}]}